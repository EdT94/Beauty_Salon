<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.ZinaBeautySalon" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/myPurple</item>
        <item name="colorPrimaryVariant">@color/myPink</item>
        <item name="colorOnPrimary">@color/black</item>
        <item name="colorSecondary">@color/myPurple</item>
        <item name="colorSecondaryVariant">@color/myPink</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>

    <style name="Theme.ZinaBeautySalon.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.ZinaBeautySalon.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.ZinaBeautySalon.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>